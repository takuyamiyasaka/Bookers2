<h2>EditBook</h2>
<p class="ti">Title</p>
<%=form_for(@book,url:"/books/#{@book.id}") do |f| %>
<h4>Title</h4>
<%=f.text_field :title %>
<h4>Body</h4>
<%=f.text_area :body %>
<div><%=f.submit "Update Book" %></div>
<p class="link"><%=link_to "Show",book_path(@book.id)%> |
<%=link_to "Back",books_path %></p>
<% end %>
